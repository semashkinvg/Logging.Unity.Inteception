<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageReleaseNotes>See release notes here  https://github.com/semashkinvg/Logging.Unity.Interception/blob/master/ReleaseNotes.md</PackageReleaseNotes>
    <PackageTags>Logging Interception Unity IoC</PackageTags>
    <RepositoryUrl>https://github.com/semashkinvg/Logging.Unity.Interception</RepositoryUrl>
    <PackageIconUrl>https://github.com/semashkinvg/Logging.Unity.Interception/blob/master/default.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/semashkinvg/Logging.Unity.Interception</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/semashkinvg/Logging.Unity.Interception/blob/master/LICENSE</PackageLicenseUrl>
    <Copyright>Copyright 2018</Copyright>
    <Description>Implementes simple logging using Unity.Interception and LibLog to support all popular logging libraries</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>1.0.1.0</AssemblyVersion>
    <FileVersion>1.0.1.0</FileVersion>
    <Version>1.0.1</Version>
    <Authors>Vladimir Semashkin</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LibLog" Version="5.0.2" />
    <PackageReference Include="Unity.Interception" Version="5.5.3" />
  </ItemGroup>

</Project>
